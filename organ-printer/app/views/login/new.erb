<!-- LOGIN NEW PAGE -->

<h4 class="container text-center">Login Form</h4>

<div class="container text-center">
  <%= form_tag '/login' do %>
    <p><%= label_tag :username %></p>
    <%= text_field_tag :username %>

    <p><%= label_tag :password %></p>
    <%= password_field_tag :password %>
    <br>
    <%= submit_tag "Login" %>
  <% end %>
</div>

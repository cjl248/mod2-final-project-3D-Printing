<!DOCTYPE html>
<html>
  <head>
    <title>OrganPrinter</title>
    <%= csrf_meta_tags %>
    <%= csp_meta_tag %>

    <%= stylesheet_link_tag    'application', media: 'all', 'data-turbolinks-track': 'reload' %>
    <%= javascript_include_tag 'application', 'data-turbolinks-track': 'reload' %>
  </head>

  <body>

    <header>
      <% if flash[:messages] %>
          <p><%= flash[:messages] %></p>
      <% end %>
    </header>

    <%= yield %>

  </body>
</html>
